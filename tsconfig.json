{
  "compilerOptions": {
      "target": "ES5",
      "lib": [
          "dom",
          "dom.iterable",
          "esnext"
      ],
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "Node",
      "jsx": "react",
      "rootDir": "src",
      "baseUrl": "src",
      "allowJs": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
  },
  "include": [
    "src"
  ]
}