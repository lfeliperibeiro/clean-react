{
  "compilerOptions": {
      "target": "ES6",
      "lib": [
          "dom",
          "dom.iterable",
          "esnext"
      ],
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "Node",
      "jsx": "react",
      "rootDir": "src",
      "baseUrl": "src",
      "paths": {
      "@/*": ["*"]
      },
      "allowJs": true,
      "resolveJsonModule": true,
  },
  "include": [
    "src"
  ]
}